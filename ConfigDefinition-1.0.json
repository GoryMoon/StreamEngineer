{
    "definitions": {
        "meteors": {
            "params": {
                "radius": {
                    "type": "decimal",
                    "display": "Radius",
                    "default": 1,
                    "step": 0.1,
                    "min": 0,
                    "description": "The distance around the player to spawn a meteor storm, 1.0 is vanilla",
                    "required": true
                },
                "amount": {
                    "type": "number",
                    "display": "Amount",
                    "default": 1,
                    "min": 1,
                    "description": "The amount of metor storms that will spawn",
                    "required": true
                }
            },
